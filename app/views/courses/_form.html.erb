
<br>

    <%= form_with(model: [@lecturer, course], local: true) do |f| %>
    <%= f.label "Subject: " %>
    <%= f.text_field :subject %><br>
    <%= f.collection_check_boxes :student_id, Student.all, :id, :last_name %>
    <%= f.hidden_field :lecturer_id, value: current_lecturer.id %>
    <%= f.submit %>
<%end%>

